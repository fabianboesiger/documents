method t1(a: Int) returns (b: Int)
    requires a > 0
    ensures b == 2 || b == 3 || b == 4 || b == 5
{
    if (a == 1) {
        b := 2
    } elseif (a == 2) {
        b := 3
    } elseif (a == 3) {
        b := 4
    } elseif (a == 4) {
        b := 2
    } else {
        b := 5
    }
}