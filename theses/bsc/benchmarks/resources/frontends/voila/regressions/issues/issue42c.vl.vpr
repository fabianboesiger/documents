method test01R1(r: Ref, lvl: Int, x: Ref)
  requires acc(R1(r, lvl, x), write) && (lvl >= 0 && R1_state(r, lvl, x) == 0) && acc(R1_G1(r), write)
  ensures acc(R1(r, lvl, x), write) && (lvl >= 0 && true)
{
  var n: Int
  var $_levelVar_0: Int
  var $_levelVar_1: Int
  var $_levelVar_2: Int
  inhale $_levelVar_0 >= 0 && $_levelVar_0 > lvl
  assert $_levelVar_0 >= 0
  inhale acc(R1_sk_fp(), write) && acc(R2_sk_fp(), write) && acc(R4_sk_fp(), write)
  
  // no init required
  
  
  
  // no init required
  
  
  
  // no init required
  
  
  inhale (forall $r: Ref, $lvl: Int, $x: Ref :: acc(R1_interferenceContext_fp($r, $lvl, $x), write))
  inhale (forall $r: Ref, $lvl: Int, $x: Ref :: acc(R2_interferenceContext_fp($r, $lvl, $x), write))
  inhale (forall $r: Ref, $lvl: Int, $x: Ref :: acc(R4_interferenceContext_fp($r, $lvl, $x), write))
  
  // ------- Inferring interference context R1,R2,R4 (beginning of non atomic procedure) BEGIN 
  
  
  label pre_infer0
  
  // Inferring interference all instances of region R1
  
  ___silicon_hack407_havoc_all_R1_interferenceContext_fp()
  exhale acc(R1_sk_fp(), write)
  inhale acc(R1_sk_fp(), write)
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R1($r, $lvl, $x)] :: (forall $$_m: Int :: { ($$_m in R1_interferenceSet_df(1, $r, $lvl, $x)) } none < old[pre_infer0](perm(R1($r, $lvl, $x))) ==> ($$_m in R1_interferenceSet_hf(1, $r, $lvl, $x)) == ((none < perm($r.$diamond) && none < perm(R1_atomicity_context_fp($r, $lvl, $x)) ==> ($$_m in R1_atomicity_context_hf($r, $lvl, $x))) && ($$_m == old[pre_infer0](R1_state($r, $lvl, $x)) || 0 == old[pre_infer0](R1_state($r, $lvl, $x)) && 1 == $$_m && true && perm(R1_G1($r)) == none))))
  
  // havoc performed by other front resource
  
  
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R1($r, $lvl, $x)] :: none < old[pre_infer0](perm(R1($r, $lvl, $x))) ==> R1_interferenceReference_hf(1, $r, $lvl, $x) == old[pre_infer0](R1_state($r, $lvl, $x)))
  
  // Inferring interference all instances of region R2
  
  ___silicon_hack407_havoc_all_R2_interferenceContext_fp()
  exhale acc(R2_sk_fp(), write)
  inhale acc(R2_sk_fp(), write)
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R2($r, $lvl, $x)] :: (forall $$_m: Int :: { ($$_m in R2_interferenceSet_df(1, $r, $lvl, $x)) } none < old[pre_infer0](perm(R2($r, $lvl, $x))) ==> ($$_m in R2_interferenceSet_hf(1, $r, $lvl, $x)) == ((none < perm($r.$diamond) && none < perm(R2_atomicity_context_fp($r, $lvl, $x)) ==> ($$_m in R2_atomicity_context_hf($r, $lvl, $x))) && ($$_m == old[pre_infer0](R2_state($r, $lvl, $x)) || R2_sk_$_action_n($r, $lvl, $x) == old[pre_infer0](R2_state($r, $lvl, $x)) && R2_sk_$_action_m($r, $lvl, $x) == $$_m && R2_sk_$_action_n($r, $lvl, $x) < R2_sk_$_action_m($r, $lvl, $x) && perm(R2_G2($r, R2_sk_$_action_n($r, $lvl, $x))) == none))))
  
  // havoc performed by other front resource
  
  
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R2($r, $lvl, $x)] :: none < old[pre_infer0](perm(R2($r, $lvl, $x))) ==> R2_interferenceReference_hf(1, $r, $lvl, $x) == old[pre_infer0](R2_state($r, $lvl, $x)))
  
  // Inferring interference all instances of region R4
  
  ___silicon_hack407_havoc_all_R4_interferenceContext_fp()
  exhale acc(R4_sk_fp(), write)
  inhale acc(R4_sk_fp(), write)
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R4($r, $lvl, $x)] :: (forall $$_m: Int :: { ($$_m in R4_interferenceSet_df(1, $r, $lvl, $x)) } none < old[pre_infer0](perm(R4($r, $lvl, $x))) ==> ($$_m in R4_interferenceSet_hf(1, $r, $lvl, $x)) == ((none < perm($r.$diamond) && none < perm(R4_atomicity_context_fp($r, $lvl, $x)) ==> ($$_m in R4_atomicity_context_hf($r, $lvl, $x))) && ($$_m == old[pre_infer0](R4_state($r, $lvl, $x)) || R4_sk_$_action_n($r, $lvl, $x) == old[pre_infer0](R4_state($r, $lvl, $x)) && R4_sk_$_action_m($r, $lvl, $x) == $$_m && R4_sk_$_action_n($r, $lvl, $x) < R4_sk_$_action_m($r, $lvl, $x) && perm(R4_G4($r, R4_sk_$_action_k($r, $lvl, $x))) == none))))
  
  // havoc performed by other front resource
  
  
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R4($r, $lvl, $x)] :: none < old[pre_infer0](perm(R4($r, $lvl, $x))) ==> R4_interferenceReference_hf(1, $r, $lvl, $x) == old[pre_infer0](R4_state($r, $lvl, $x)))
  
  // ------- Inferring interference context R1,R2,R4 (beginning of non atomic procedure) END 
  
  
  
  // ------- assume BEGIN ------------
  
  
  inhale 0 <= n && n <= 1
  
  // ------- assume END --------------
  
  
  
  // ------- use-atomic BEGIN --------
  
  
  label pre_use_atomic0
  assert perm(R1_atomicity_context_fp(r, lvl, x)) == none
  assert $_levelVar_0 > lvl
  $_levelVar_1 := lvl
  exhale acc(R1_G1(r), write)
  
  // ------- Stabilising regions R2,R4 (before use-atomic@20.3) BEGIN 
  
  
  label pre_stabilize0
  
  // Stabilising all instances of region R2
  
  ___silicon_hack407_havoc_all_R2()
  exhale acc(R2_sk_fp(), write)
  inhale acc(R2_sk_fp(), write)
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R2($r, $lvl, $x)] :: none < old[pre_stabilize0](perm(R2($r, $lvl, $x))) ==> (none < perm($r.$diamond) && none < perm(R2_atomicity_context_fp($r, $lvl, $x)) ==> (R2_state($r, $lvl, $x) in R2_atomicity_context_hf($r, $lvl, $x))) && (R2_state($r, $lvl, $x) == old[pre_stabilize0](R2_state($r, $lvl, $x)) || R2_sk_$_action_n($r, $lvl, $x) == old[pre_stabilize0](R2_state($r, $lvl, $x)) && R2_sk_$_action_m($r, $lvl, $x) == R2_state($r, $lvl, $x) && R2_sk_$_action_n($r, $lvl, $x) < R2_sk_$_action_m($r, $lvl, $x) && perm(R2_G2($r, R2_sk_$_action_n($r, $lvl, $x))) == none))
  
  // Stabilising all instances of region R4
  
  ___silicon_hack407_havoc_all_R4()
  exhale acc(R4_sk_fp(), write)
  inhale acc(R4_sk_fp(), write)
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R4($r, $lvl, $x)] :: none < old[pre_stabilize0](perm(R4($r, $lvl, $x))) ==> (none < perm($r.$diamond) && none < perm(R4_atomicity_context_fp($r, $lvl, $x)) ==> (R4_state($r, $lvl, $x) in R4_atomicity_context_hf($r, $lvl, $x))) && (R4_state($r, $lvl, $x) == old[pre_stabilize0](R4_state($r, $lvl, $x)) || R4_sk_$_action_n($r, $lvl, $x) == old[pre_stabilize0](R4_state($r, $lvl, $x)) && R4_sk_$_action_m($r, $lvl, $x) == R4_state($r, $lvl, $x) && R4_sk_$_action_n($r, $lvl, $x) < R4_sk_$_action_m($r, $lvl, $x) && perm(R4_G4($r, R4_sk_$_action_k($r, $lvl, $x))) == none))
  
  // ------- Stabilising regions R2,R4 (before use-atomic@20.3) END 
  
  
  unfold acc(R1(r, lvl, x), write)
  
  // no interference context translation needed
  
  
  
  // ------- Stabilising regions R1 (before use-atomic@20.3) BEGIN 
  
  
  label pre_stabilize
  
  // Stabilising all instances of region R1
  
  ___silicon_hack407_havoc_all_R1()
  exhale acc(R1_sk_fp(), write)
  inhale acc(R1_sk_fp(), write)
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R1($r, $lvl, $x)] :: none < old[pre_stabilize](perm(R1($r, $lvl, $x))) ==> (none < perm($r.$diamond) && none < perm(R1_atomicity_context_fp($r, $lvl, $x)) ==> (R1_state($r, $lvl, $x) in R1_atomicity_context_hf($r, $lvl, $x))) && (R1_state($r, $lvl, $x) == old[pre_stabilize](R1_state($r, $lvl, $x)) || 0 == old[pre_stabilize](R1_state($r, $lvl, $x)) && 1 == R1_state($r, $lvl, $x) && true && perm(R1_G1($r)) == none))
  
  // ------- Stabilising regions R1 (before use-atomic@20.3) END 
  
  
  inhale acc(R1_G1(r), write)
  
  // ------- heap-write BEGIN --------
  
  
  x.$cell_$value := n
  
  // ------- heap-write END ----------
  
  
  fold acc(R1(r, lvl, x), write)
  assert old[pre_use_atomic0](R1_state(r, lvl, x)) == R1_state(r, lvl, x) || 0 == old[pre_use_atomic0](R1_state(r, lvl, x)) && 1 == R1_state(r, lvl, x)
  $_levelVar_2 := $_levelVar_0
  
  // ------- use-atomic END ----------
  
  
  
  // ------- Stabilising regions R1,R2,R4 (after seq-comp@18.3) BEGIN 
  
  
  label pre_stabilize2
  
  // Stabilising all instances of region R1
  
  ___silicon_hack407_havoc_all_R1_interferenceContext_fp()
  exhale acc(R1_sk_fp(), write)
  inhale acc(R1_sk_fp(), write)
