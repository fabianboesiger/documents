  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R2($r, $lvl, $x)] :: none < old[pre_stabilize24](perm(R2($r, $lvl, $x))) ==> (none < perm($r.$diamond) && none < perm(R2_atomicity_context_fp($r, $lvl, $x)) ==> (R2_state($r, $lvl, $x) in R2_atomicity_context_hf($r, $lvl, $x))) && (R2_state($r, $lvl, $x) == old[pre_stabilize24](R2_state($r, $lvl, $x)) || R2_sk_$_action_n($r, $lvl, $x) == old[pre_stabilize24](R2_state($r, $lvl, $x)) && R2_sk_$_action_m($r, $lvl, $x) == R2_state($r, $lvl, $x) && R2_sk_$_action_n($r, $lvl, $x) < R2_sk_$_action_m($r, $lvl, $x) && perm(R2_G2($r, R2_sk_$_action_n($r, $lvl, $x))) == none))
  
  // Stabilising all instances of region R4
  
  ___silicon_hack407_havoc_all_R4()
  exhale acc(R4_sk_fp(), write)
  inhale acc(R4_sk_fp(), write)
  inhale (forperm $r: Ref, $lvl: Int, $x: Ref [R4($r, $lvl, $x)] :: none < old[pre_stabilize24](perm(R4($r, $lvl, $x))) ==> (none < perm($r.$diamond) && none < perm(R4_atomicity_context_fp($r, $lvl, $x)) ==> (R4_state($r, $lvl, $x) in R4_atomicity_context_hf($r, $lvl, $x))) && (R4_state($r, $lvl, $x) == old[pre_stabilize24](R4_state($r, $lvl, $x)) || R4_sk_$_action_n($r, $lvl, $x) == old[pre_stabilize24](R4_state($r, $lvl, $x)) && R4_sk_$_action_m($r, $lvl, $x) == R4_state($r, $lvl, $x) && R4_sk_$_action_n($r, $lvl, $x) < R4_sk_$_action_m($r, $lvl, $x) && perm(R4_G4($r, R4_sk_$_action_k($r, $lvl, $x))) == none))
  
  // ------- Stabilising regions R1,R2,R4 (check stability of method condition) END 
  
  
  assert acc(R4(r, lvl, x), write) && (lvl >= 0 && true) && (R4_G4_T(r, R4_state(r, lvl, x)) && acc(R4_G4(r, R4_state(r, lvl, x)), write))
